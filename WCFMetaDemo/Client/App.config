<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="EmployeeServiceEndpoint" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8989/EmployeeService" binding="basicHttpBinding"
                bindingConfiguration="EmployeeServiceEndpoint" contract="EmployeeService.IEmployeeService"
                name="EmployeeServiceEndpoint" />
        </client>
    </system.serviceModel>
</configuration>